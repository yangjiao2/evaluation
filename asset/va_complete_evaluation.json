{
  "EvaluationSchema": {
    "Notification": {
      "EmailRecipients": [
        "mvijay@nvidia.com",
        "yangj@nvidia.com"
      ]
    },
    "NemoEvaluator": {
      "DatasetConfig": {
        "Engine": "local"
      },
      "Evaluators": [
        {
          "name": "llm_evaluation",
          "column_map": {
            "question": "Query",
            "reference": "Correct Answer",
            "answer": "Response",
            "category": "Source",
            "additional_params": [
              "Correct Answer",
              "Required Citations",
              "Short Answer Expected"
            ]
          },
          "model": {
            "llm_name": "mixtral-8x22b"
          },
          "evaluator_payload": {
            "eval_type": "llm_as_a_judge",
            "eval_subtype": "mtbench",
            "mode": "single",
            "judge_inference_params": {
              "top_p": 0.1,
              "top_k": 40,
              "temperature": 0,
              "stop": [],
              "tokens_to_generate": 6000
            },
            "inference_params": {
              "tokens_to_generate": 6000,
              "temperature": 0,
              "top_k": 1,
              "top_p": 0.75,
              "stop": [],
              "extra_body": {
                "project_id": 11,
                "env": "sandbox",
                "system": "nvinfo",
                "model": "mixtral_agent"
              }
            }
          },
          "judge_config": {
            "prompt_module": "eval_prompt_library.metrics_eval_prompt.MetricsEvaluationPrompt",
            "output_format": "output_format",
            "template": "eval_template_required_citations_short_answer_expected",
            "scorers": [
              "Correctness Answer",
              "Helpfulness",
              "Conciseness"
            ],
            "parse_keys": [
              "Explanation"
            ]
          },
          "run_config": {
            "Outputs": [
              {
                "type": "Attribute",
                "value": ""
              },
              {
                "type": "Function",
                "value": "service_library.run.parser_library.va_parser.citation_score"
              }
            ]
          }
        }
      ]
    }
  },
  "RegressionSchema": {
    "DatasetConfig": {
      "Engine": "s3",
      "RunFile": "",
      "Name": "va",
      "DatasetPath": "dataset/nava.xlsx"
    },
    "DataConfigs": {
      "column_map": {
        "question": "Query",
        "reference": "Correct Answer",
        "category": "Source"
      }
    },
    "RunConfig": {
      "Inputs": [
        {
          "name": "query",
          "type": "Attribute",
          "value": "Query"
        },
        {
          "name": "userId",
          "type": "Attribute",
          "value": "userId"
        },
        {
          "name": "emailId",
          "type": "Attribute",
          "value": "Email"
        }
      ],
      "Outputs": [
        {
          "name": "Result",
          "type": "Attribute",
          "value": "result"
        },
        {
          "name": "",
          "type": "Function",
          "value": "service_library.run.parser_library.va_parser.extract_va_response"
        }
      ]
    }
  }
}